    "react-dom": "18.2.0",
    "react-icons": "4.4.0",
    "react-intl": "6.0.5",
    "react-query": "3.39.1",
    "start-server-and-test": "1.14.0",
    "superjson": "1.9.1",
    "zod": "3.17.3"
