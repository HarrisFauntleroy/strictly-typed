{
  "name": "strongly-typed",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:1-migrate": "prisma migrate deploy",
    "build:2-build": "next build",
    "build": "run-s build:*",
    "db-up": "docker context use home-server && docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "yarn prisma migrate dev",
    "db:reset": "docker-compose down --volumes --remove-orphans",
    "dev": "next dev",
    "trunk:fix": " trunk check --all --fix",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "start": "next start",
    "prettier": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
    "prettier:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
    "pre-commit": "lint-staged",
    "lint": "next lint",
    "eslint": "eslint \"**/*.+(js|jsx|ts|tsx)\"",
    "eslint:fix": "eslint --fix \"**/*.+(js|jsx|ts|tsx)\"",
    "test:dev": "start-server-and-test dev 3000 test",
    "test:start": "start-server-and-test start 3000 test",
    "test": "run-s test:*",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "postinstall": "prisma generate"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.4",
    "@chakra-ui/react": "^2.2.4",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "4.0.0",
    "@trpc/client": "^9.26.0",
    "@trpc/next": "^9.26.0",
    "@trpc/react": "^9.26.0",
    "@trpc/server": "^9.26.0",
    "@uiw/react-md-editor": "3.6.0",
    "clsx": "^1.1.1",
    "framer-motion": "^6.5.1",
    "katex": "^0.16.0",
    "mermaid": "^9.1.3",
    "next": "^12.1.6",
    "next-auth": "^4.10.0",
    "next-remove-imports": "^1.0.6",
    "nodemailer": "^6.7.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-focus-lock": "^2.9.1",
    "react-hook-form": "^7.33.1",
    "react-icons": "^4.4.0",
    "react-intl": "^6.0.5",
    "react-markdown": "^8.0.3",
    "react-query": "^3.37.0",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-katex": "^6.0.2",
    "rehype-stringify": "^9.0.3",
    "remark-breaks": "^3.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.1",
    "remarkable": "^2.0.1",
    "start-server-and-test": "^1.12.0",
    "superjson": "^1.7.4",
    "unified": "^10.1.2",
    "uuidv4": "^6.2.13",
    "ws": "^8.8.1",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.23.4",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^28.1.6",
    "@types/mermaid": "^8.2.9",
    "@types/node": "^17.0.10",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^18.0.9",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "8.20.0",
    "eslint-config-next": "^12.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "jest": "^27.1.0",
    "npm-run-all": "^4.1.5",
    "playwright": "1.23.4",
    "prettier": "^2.5.1",
    "prisma": "4.0.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.3.0",
    "typescript": "4.7.4"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  }
}
